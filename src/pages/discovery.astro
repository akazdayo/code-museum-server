---
import { sql } from "drizzle-orm";
import CodeViewer from "../components/CodeViewer.astro";
import { db } from "../db";
import Layout from "../layouts/Layout.astro";
import { codeTable } from "../schema";

const ranking = await db
	.select()
	.from(codeTable)
	.orderBy(sql`${codeTable.likes} desc`);
---

<Layout title="発見">
	<CodeViewer />
</Layout>
